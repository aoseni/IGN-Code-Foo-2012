import java.util.Scanner;
import java.util.Vector;
import java.util.Collections;
import java.util.Comparator;
public class LicencePlate {
  Vector<Integer> v = new Vector();
	public LicencePlate(String value){
		for(int i = 0; i<value.length(); i++){
			for(int j = 0; j<value.length(); j++){
				
				this.v.add((int)(Math.pow(26, i)*Math.pow(10, j)));
				Collections.sort(v);
			}
		}
		
	}
	public int LowestAmountOfPlates(int copy){
		int returner = 0;
		int small=0;
		int v=0;
		for(int n= 0; n<this.v.size()-1;n++){
		if(this.v.elementAt(n)-copy>=0){
			v=n;
			small = this.v.elementAt(n)-copy;
			break;
		}
		}
		for(int h = v; h<this.v.size()-1;h++){
			small = Math.min(small, this.v.get(h)-copy);
		}
		for(int a= 0; a<this.v.size()-1;a++){
			if(small == this.v.elementAt(a)-copy){
				returner = this.v.elementAt(a);
			}
		}
		return returner;
	}
	
	
	public boolean isAPowerOfTwentySix(String value){
		boolean yes = false;
		int power = value.length();
		for(int n = 0;n<=power; n++){
			if(Integer.parseInt(value)/Math.pow(26, 1.0+n)==1.0){
				yes = true;
			}
		}
		return yes;
	}
	public boolean isAPowerOfTen(String value){
		boolean yes = false;
		int power = value.length();
		
			if(value.charAt(power-1)=='0'){
				yes = true;
			
		}
		return yes;
	}
	public static void main(String args[]){
		
	System.out.println("Enter population: ");
	Scanner input = new Scanner(System.in);
	String population = input.next();
	LicencePlate lp = new LicencePlate(population);
	int copy = Integer.parseInt(population);
	
	int letter = 0;
	int number = 0;
	int roger = lp.LowestAmountOfPlates(copy);
	int copythat = roger;
	int excess = roger-copy;
	
	while(roger>=10){
		 if(lp.isAPowerOfTwentySix((Integer.toString(roger)))){
			 letter++;
			 roger= roger/26;
			 
		 }else
			 if(lp.isAPowerOfTen((Integer.toString(roger)))){
			 number++;
			roger= roger/10;
			
		 }
	}
		if(copy<10 && copy>=0){
			number++;
			excess = 10-copy;
		}
	System.out.println("Population: "+copy +"\n"+
						"Pattern: "+letter+" letters, "+number+" numbers"+
						"\n"+"Total Plates: "+copythat+
						"\n"+"Excess Plates: "+excess);
	
	}
}
